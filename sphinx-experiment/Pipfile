[scripts]
clean="rm -rf build"
docs-html="sphinx-build -b html docs build/html"
docs-singlehtml="sphinx-build -b singlehtml docs build/singlehtml"
docs-epub="sphinx-build -b epub docs build/epub"
docs-text="sphinx-build -b text docs build/text"
